---
export interface Props { title?: string }
const { title } = Astro.props;
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>{title || "Verifiable News"}</title>
  </head>
  <body>
    <main><slot /></main>
  </body>
</html>